{
  "name": "lovex-app",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "start:simple": "copy App.simple.js App.js && expo start --clear --port 9032",
    "start:full": "copy App.full.js App.js && expo start --clear --port 9033",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "reset": "expo start -c",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings 0",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "build:staging": "npx eas build --platform all --profile staging --non-interactive",
    "build:production": "npx eas build --platform all --profile production --non-interactive",
    "submit:staging": "npx eas submit --platform all --profile staging --non-interactive",
    "submit:production": "npx eas submit --platform all --profile production --non-interactive",
    "performance-test": "node scripts/performance-test.js",
    "integration-test": "node scripts/integration-test.js"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-navigation/bottom-tabs": "^7.0.0",
    "@react-navigation/native": "^7.0.0",
    "@react-navigation/stack": "^7.0.0",
    "@supabase/supabase-js": "^2.84.0",
    "@tanstack/react-query": "^5.90.11",
    "expo": "~54.0.27",
    "expo-av": "~16.0.8",
    "expo-blur": "~15.0.8",
    "expo-camera": "~17.0.10",
    "expo-device": "~8.0.10",
    "expo-file-system": "~19.0.20",
    "expo-haptics": "~15.0.8",
    "expo-image": "~3.0.11",
    "expo-image-manipulator": "~14.0.8",
    "expo-image-picker": "~17.0.9",
    "expo-linear-gradient": "~15.0.8",
    "expo-local-authentication": "~17.0.8",
    "expo-location": "~19.0.8",
    "expo-notifications": "~0.32.14",
    "expo-secure-store": "~15.0.8",
    "expo-status-bar": "~3.0.9",
    "ffmpeg-kit-react-native": "^6.0.2",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-encrypted-storage": "^4.0.3",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-maps": "1.20.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-webview": "13.15.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.0",
    "@babel/eslint-parser": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@perf-profiler/profiler": "^0.10.11",
    "@react-native-community/cli": "^20.0.2",
    "@react-native-community/eslint-config": "^3.2.0",
    "@react-native/metro-config": "^0.82.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/react": "^19.2.7",
    "babel-jest": "^30.2.0",
    "babel-preset-expo": "^54.0.7",
    "eslint": "^8.57.1",
    "fast-check": "^4.3.0",
    "jest": "^30.2.0",
    "k6": "^0.0.0",
    "lighthouse": "^13.0.1",
    "react-native-bundle-visualizer": "^3.1.3",
    "react-test-renderer": "^19.1.0",
    "supabase": "^2.65.6",
    "webpack-bundle-analyzer": "^5.0.1"
  },
  "private": true
}
